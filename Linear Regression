    "import statistics as stats\n",
    "data =[ 2,5,8,4,9,6,4]\n",
    "mean = stats.mean(data)\n",
    "median=stats.median(data)\n",
    "mode= stats.mode(data)\n",
    "print(f\"mean:{mean}\")\n",
    "print(f\"mode:{mode}\")\n",
    "print(f\"median:{median}\")\n"
